            steps {
                echo 'hhhhhhhhhhhh'
            }
        }
        stage('kkkk') {
            steps {
                echo 'kkkkkkkkkkkkkk'
            }
        }
       stage('Hello-1') {
            steps {
                echo 'Hello World'
            }
        }
        stage('hai-1') {
